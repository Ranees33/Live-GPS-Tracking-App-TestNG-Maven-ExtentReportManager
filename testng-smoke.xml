<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Smoke_Suite">
	
	<listeners>
		<listener class-name="testCases.Listener" />
	</listeners>
	
    <parameter name="browser" value="chrome"/>
    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="testCases"/>
        </packages>
    </test>
</suite>